    a:layout_width="match_parent"
    a:layout_height="match_parent">

	<TextView
		a:text="bash"
		a:textAppearance="?android:attr/textAppearanceLarge"
		a:layout_width="match_parent"
		a:layout_height="wrap_content"
		a:minWidth="25px"
		a:minHeight="25px"
		a:id="@+id/temperature" />
	<TextView
		a:text="bash"
		a:textAppearance="?android:attr/textAppearanceLarge"
		a:layout_width="match_parent"
		a:layout_height="wrap_content"
		a:minWidth="25px"
		a:minHeight="25px"
		a:id="@+id/minimumtemperature" />
	<TextView
		a:text="bash"
		a:textAppearance="?android:attr/textAppearanceLarge"
		a:layout_width="match_parent"
		a:layout_height="wrap_content"
		a:minWidth="25px"
		a:minHeight="25px"
		a:id="@+id/maximumtemperature" />

</LinearLayout>